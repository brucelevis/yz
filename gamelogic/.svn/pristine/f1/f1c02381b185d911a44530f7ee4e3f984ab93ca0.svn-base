			{ npc1 = 0, type = 4, emoji1 = 0, name = "爱拉忒", talk = 109020, npc2 = 0, emoji2 = 0, content = "但他林大人,躲躲藏藏不会太无聊了吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


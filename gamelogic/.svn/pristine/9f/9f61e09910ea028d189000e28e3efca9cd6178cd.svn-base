	[1092] = {
		texts = {
			
			{ npc1 = 109015, type = 1, emoji1 = 0, name = "莱伦", talk = 109015, npc2 = 0, emoji2 = 0, content = "总的来说还不错。不过千万不要因此而沾沾自喜。我的同僚布洛姆可不是个友善的家伙。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },



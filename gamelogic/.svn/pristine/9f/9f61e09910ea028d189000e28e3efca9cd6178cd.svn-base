	[1082] = {
		texts = {
			
			{ npc1 = 108025, type = 1, emoji1 = 0, name = "莱伦", talk = 108025, npc2 = 0, emoji2 = 0, content = "什么？是艾格娅让你来的？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 108025, type = 1, emoji1 = 0, name = "莱伦", talk = 108025, npc2 = 0, emoji2 = 0, content = "那个孩子啊……虽然看起来天真无拘,其实无论对他人还是自己,要求都十分严苛呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 108025, type = 1, emoji1 = 0, name = "莱伦", talk = 108025, npc2 = 0, emoji2 = 0, content = "所以,你一定是一位非常优秀的勇者吧。既然如此,下面的话我只说一次哦。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 108025, type = 1, emoji1 = 0, name = "莱伦", talk = 108025, npc2 = 0, emoji2 = 0, content = "（介绍技能二）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 108025, type = 1, emoji1 = 0, name = "莱伦", talk = 108025, npc2 = 0, emoji2 = 0, content = "听明白了吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 108025, type = 1, emoji1 = 0, name = "莱伦", talk = 108025, npc2 = 0, emoji2 = 0, content = "我的帮手缔诺会帮你快速掌握这个技能的。拿起武器迎战吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


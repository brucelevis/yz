data_1500_MainTaskText = {

	[11] = {
		texts = {
			
			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "永夜即将降临,圣怒何以平息", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "吾,迦南卑微之臣,虔诚祈求", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "无所不能的先王所罗门陛下", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "听到臣民恳切的呼唤", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "愿您从异世无尽的长眠中苏醒", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "带领您的子民,开辟希望与光明的出路", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "这里是什么地方……刚才……我……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "奇怪,怎么想不起来了……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "米利暗", talk = 1010, npc2 = 0, emoji2 = 0, content = "由传送之门而来的异界勇者吗？不错,很有活力的年轻人啊。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "米利暗", talk = 1010, npc2 = 0, emoji2 = 0, content = "我是这里的大祭司米利暗,有什么问题的话,阁下可以尽管问我。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "对不起,请问……这到底是怎么一回事？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "米利暗", talk = 1010, npc2 = 0, emoji2 = 0, content = "唉……这件事实在不宜声张,不过作为肩负着拯救迦南的使命的勇者,知道这些也是必须的。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "米利暗", talk = 1010, npc2 = 0, emoji2 = 0, content = "在如你所见的这座迦南的圣殿之中,原有两件令人忌惮的相克宝物。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "米利暗", talk = 1010, npc2 = 0, emoji2 = 0, content = "其一是我们的先祖与神定下的契约,其二是四百余年前,先圣王所罗门陛下降服并封印的魔神。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "魔神？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "米利暗", talk = 1010, npc2 = 0, emoji2 = 0, content = "是的,这些魔神,有七十二位之多。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "米利暗", talk = 1010, npc2 = 0, emoji2 = 0, content = "如果这个平衡没有被打破,迦南王国也许会一直平静下去。然而近日存放契约的圣约柜竟离奇消失。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "失去了制约的魔神们很快突破了封印,逃亡各地作乱,以致民不聊生。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[12] = {
		texts = {
			
			{ npc1 = 1010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "原来是这样啊……那么,我又是怎么到这里的呢？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 1010, type = 1, emoji1 = 0, name = "米利暗", talk = 1010, npc2 = 0, emoji2 = 0, content = "圣王所罗门陛下曾有遗训：于迦南危急时刻开启传送之门,陛下将重现在这片土地上,带领我们开辟希望之路。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[21] = {
		texts = {
			
			{ npc1 = 2010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "愿您从异世无尽的长眠中……苏醒……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 2010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "带领子民,开辟希望与光明的出路。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 2010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "那个人……祭司大人就是您吧？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 2010, type = 1, emoji1 = 0, name = "米利暗", talk = 2010, npc2 = 0, emoji2 = 0, content = "请叫我米利暗吧。虽然是大祭司,也不能承受被勇者们称作“大人”呢？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 2010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "那么,圣谕祭司说的所罗门王的转世“之一”又是怎样一回事呢？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 2010, type = 1, emoji1 = 0, name = "米利暗", talk = 2010, npc2 = 0, emoji2 = 0, content = "在传送之门开启的瞬间,圣殿里出现了数百位勇者。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 2010, type = 1, emoji1 = 0, name = "米利暗", talk = 2010, npc2 = 0, emoji2 = 0, content = "很抱歉,究竟为何如此目前仍无法明确,或许有某一位勇者大人是真正所罗门陛下的转世,或许大家都分别承担了陛下的力量,目前而言仍无法确定。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 2010, type = 1, emoji1 = 0, name = "米利暗", talk = 2010, npc2 = 0, emoji2 = 0, content = "不过,如果是得遇机缘的勇者大人的话,一定会找出导致这个情况的原因吧！", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 2010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "可是……就算是我这样从未接受过训练的人,也能成为一名合格的勇者吗？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 2010, type = 1, emoji1 = 0, name = "米利暗", talk = 2010, npc2 = 0, emoji2 = 0, content = "别担心,所谓勇者自然离不开冒险与战斗,通过不断地努力,一点一点强大起来。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[22] = {
		texts = {
			
			{ npc1 = 2010, type = 1, emoji1 = 0, name = "米利暗", talk = 2010, npc2 = 0, emoji2 = 0, content = "这样的改变可不是一朝一夕完成的,但是勇者大人的话,我相信不会有什么问题。而且,我的助手艾格娅也会竭尽全力辅助勇者们的。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[31] = {
		texts = {
			
			{ npc1 = 3020, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "那个……请问", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 3020, type = 1, emoji1 = 0, name = "初心者", talk = 3020, npc2 = 0, emoji2 = 0, content = "“那个那个”的烦死人了！", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 3020, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "对不起,请问,艾格娅大人是哪一位？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 3010, type = 1, emoji1 = 0, name = "艾格娅", talk = 3010, npc2 = 0, emoji2 = 0, content = "是有人叫我吗？久等啦,年轻的勇者们。你们的勇气真是令人赞赏,现在开始训练吧！", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 3010, type = 1, emoji1 = 0, name = "艾格娅", talk = 3010, npc2 = 0, emoji2 = 0, content = "作为迦南王国的吉祥物,【吉祥物】们拥有可爱形态的同时,也具有强大的灵力哟。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 3010, type = 1, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "别小瞧了这些小家伙,虽然是刚刚加入的勇者,输给这群小家伙也是很难看的。所以,组队进行挑战吧。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 3010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "组队挑战？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 3010, type = 1, emoji1 = 0, name = "艾格娅", talk = 3010, npc2 = 0, emoji2 = 0, content = "是的。在遇到强大的敌人时,向可以信赖的人寻求帮助不失为一种聪明的做法。一意孤行逞匹夫之勇,只会使人陷入危险之中。因此,结识朋友也是勇者们的必修课之一。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 3010, type = 1, emoji1 = 0, name = "艾格娅", talk = 3010, npc2 = 0, emoji2 = 0, content = "准备好了吗？挑战马上开始了噢！", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 3010, type = 6, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "（激烈的战斗过后……）", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[32] = {

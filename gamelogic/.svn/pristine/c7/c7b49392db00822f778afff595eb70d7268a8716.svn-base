	[1051] = {
		texts = {
			
			{ npc1 = 105031, type = 1, emoji1 = 0, name = "思莉嘉", talk = 105031, npc2 = 0, emoji2 = 0, content = "我……我可以和你们在一起吗？我能照顾好自己。", },


			{ npc1 = 105031, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "我们要去的地方,要做的事都是非常凶险的。你还小,应该得到更好的照顾和教育。", },


			{ npc1 = 105011, type = 2, emoji1 = 0, name = "伊菲", talk = 105011, npc2 = 105021, emoji2 = 0, content = "是啊！说不定以后能成为比我们更强的勇者呢！", },


			{ npc1 = 105031, type = 1, emoji1 = 0, name = "思莉嘉", talk = 105031, npc2 = 0, emoji2 = 0, content = "好吧……", },


			{ npc1 = 105011, type = 2, emoji1 = 0, name = "爱拉忒", talk = 105021, npc2 = 105021, emoji2 = 0, content = "真的很抱歉……", },


			{ npc1 = 105041, type = 1, emoji1 = 0, name = "嘉卡", talk = 105041, npc2 = 0, emoji2 = 0, content = "哈哈,又是你！", },


			{ npc1 = 105041, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "嘉卡大人？", },


			{ npc1 = 105041, type = 1, emoji1 = 0, name = "嘉卡", talk = 105041, npc2 = 0, emoji2 = 0, content = "上次有米利暗包庇你,这次,我看谁来救你！", },


			{ npc1 = 105011, type = 2, emoji1 = 0, name = "伊菲", talk = 105011, npc2 = 105021, emoji2 = 0, content = "怎么,想打群架吗？", },


			{ npc1 = 105011, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 105021, emoji2 = 0, content = "这是我和嘉卡大人的个人恩怨,你们不要插手。", },


			{ npc1 = 105011, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 105021, emoji2 = 0, content = "这位大人上次没亲自尝到我的拳头,恐怕心里十分遗憾呢。", },


			{ npc1 = 105041, type = 1, emoji1 = 0, name = "嘉卡", talk = 105041, npc2 = 0, emoji2 = 0, content = "别说大话,我今天可是带足了打手。", },


			{ npc1 = 105041, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "是吗？那就请您亲自验证,我是不是说大话。", },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "伊菲", talk = 201020, npc2 = 201020, emoji2 = 0, content = "看样子,该不会是再吃、那个……", option1 = "", option2 = "", option3 = "", option4 = "", },



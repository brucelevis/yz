return {
	p = "friend",
	si = 6000, -- [6000,7000)
	src = [[

# 全量更新简介数据(简介数据可以用于好友列表，申请列表等)
friend_sync 6000 {
	request {
		base 0 : basetype
		srvname 1 : string
		pid 2 : integer
		name 3 : string
		roletype 4 : integer
		lv 5 : integer
	}
}

# 新增部分关系:好友/申请者/关注列表
friend_addlist 6001 {
	request {
		base 0 : basetype
		pids 1 : *integer
		# 0--applayer; 1--friend; 2--toapply
		type 2 : integer
		# true--新增的列表，false--原有列表；上线时会发送原有列表，新增列表，后续增加关系都是发新增列表
		newflag 3 : boolean
	}
}

#删除部分关系:好友/申请者/关注列表
friend_dellist 6002 {
	request {
		base 0 : basetype
		pids 1 : *integer
		# 0--applayer; 1--friend; 2--toapply
		type 2 : integer
	}
}

# 好友间私聊消息
friend_addmsgs 6003 {
	request {
		base 0 : basetype
		pid 1 : integer
		msgs 2 : *string
	}
}
]]
}

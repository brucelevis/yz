	[3071] = {
		texts = {
			
			{ npc1 = 307010, type = 1, emoji1 = 0, name = "爱拉忒", talk = 307020, npc2 = 307020, emoji2 = 0, content = "好吧……相信魔神中也存有仁爱的传言的确是我的错。赛共大人只是一个贪生怕死,罔顾他人性命的缩头乌龟罢了！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307010, type = 2, emoji1 = 0, name = "伊菲", talk = 307010, npc2 = 307020, emoji2 = 0, content = "……爱拉忒酱……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 307020, emoji2 = 0, content = "这样说……似乎有些太失礼了……爱拉忒酱。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "呵呵,是吗……小娃娃,年幼无知的你,又能明白什么？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 307020, npc2 = 307020, emoji2 = 0, content = "责任和力量相配,即使是被赛共大人视为年幼无知的我也明白这个道理。而您却选择了逃避退缩！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "如此断言,不过是因为你那被华利弗打上契印的小朋友活命无望罢了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 307020, npc2 = 307020, emoji2 = 0, content = "是！赛共大人肯帮忙的话,思莉嘉一定能安然无事的。可现在……因为心中无聊的畏惧而置他人生死于不顾,赛共大人难道不是缩头乌龟吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "迦南人果然是一如既往的无耻呢……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "在别人身上烙下契印的人,诱惑他人犯罪然后残忍毁灭的人,你难道不该去怪它？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "华利弗为什么找上赫麦村,诸位应该心理清楚吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 307020, npc2 = 307020, emoji2 = 0, content = "思莉嘉只是个孩子！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "没有因为年幼而放过她难道是我的错吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "在抱怨别人前,是否有应该反思作为朋友,却未能保护好她的安全的自己过于无能了呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "伊菲", talk = 307010, npc2 = 307010, emoji2 = 0, content = "如此费心请赛共大人出面施以援手,应该算是很够朋友了吧！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "伊菲", talk = 307010, npc2 = 307010, emoji2 = 0, content = "即使实力尚不足以保护所有人的安全,败在魔神手下难道是我们罪过吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307010, emoji2 = 0, content = "真是让人感动啊。若非面这个冷血的我,想必此时已经被自己的努力感染得涕泪横流了吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 307020, npc2 = 307020, emoji2 = 0, content = "荒唐至极！你的生命,还有无上的尊荣,这些荣耀从何而来,难道你都不记得了吗？赛共大人！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "迦南人,我赛共自然不会忘记,你所谓的尊荣,还有那绝口不提的毁灭。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 307020, npc2 = 307020, emoji2 = 0, content = "你说什么……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "勾结华利弗,并设计毁掉自己曾奉拜的神明的护体这种伎俩,他们当然不会告诉万世子孙。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "而那个名叫赛共的魔神,只不过是自己堕落罢了,和迦南人可没有半点关系。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 307020, npc2 = 307020, emoji2 = 0, content = "这些……这些话到底是什么意思……？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "说起迦南人赐予的荣耀,他们自己有何尝不是无时不在享受创造这尊荣的骄傲？！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 307020, emoji2 = 0, content = "爱拉忒酱小心！赛共有些失控了！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 307030, type = 2, emoji1 = 0, name = "赛共", talk = 307030, npc2 = 307020, emoji2 = 0, content = "如果知道当年发生的事情,就会明白今天要求是多么的无理取闹！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 6, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "（勇者们周围忽然出现了奇怪的景象,似乎是魔神赛共曾经的经历。激烈的战斗过后……）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },

		},
	},


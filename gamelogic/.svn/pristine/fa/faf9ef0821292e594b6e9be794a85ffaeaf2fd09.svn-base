	[52] = {
		texts = {
			
			{ npc1 = 5010, type = 1, emoji1 = 0, name = "米利暗", talk = 5010, npc2 = 0, emoji2 = 0, content = "做出了选择的话,就坚定不移地走下去吧！前往勇者登记处办理登记之后,就是一名正式的【职业】了。", option1 = "", option2 = "", option3 = "", option4 = "", },


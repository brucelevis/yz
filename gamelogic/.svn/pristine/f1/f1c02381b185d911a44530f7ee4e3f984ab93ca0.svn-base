			{ npc1 = 0, type = 6, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "（赫麦村的村长出面了,也是一位厉害的角色啊……）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


	[1061] = {
		texts = {
			
			{ npc1 = 106015, type = 1, emoji1 = 0, name = "艾格娅", talk = 106015, npc2 = 0, emoji2 = 0, content = "真是令人赞叹啊！只是被大祭司大人点拨了一下就如此出色的勇者,很是少见呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 106015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "被艾格娅大人这样夸奖真是不好意思呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 106015, type = 1, emoji1 = 0, name = "艾格娅", talk = 106015, npc2 = 0, emoji2 = 0, content = "请不要这样说。那么就让我为您讲解基本的格斗技巧吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 106015, type = 1, emoji1 = 0, name = "艾格娅", talk = 106015, npc2 = 0, emoji2 = 0, content = "（介绍技能一）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 106015, type = 1, emoji1 = 0, name = "艾格娅", talk = 106015, npc2 = 0, emoji2 = 0, content = "仅仅了解是远远不够的,请勇者大人在实战中熟悉一下这个技能吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


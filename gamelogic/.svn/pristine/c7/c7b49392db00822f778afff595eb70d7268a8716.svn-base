	[1061] = {
		texts = {
			
			{ npc1 = 106031, type = 1, emoji1 = 0, name = "思莉嘉", talk = 106031, npc2 = 0, emoji2 = 0, content = "是不是我又连累你们了？", },


			{ npc1 = 106031, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "怎么会！小小年纪,不要想那么多,会长不高的。", },


			{ npc1 = 106031, type = 2, emoji1 = 0, name = "中年妇女", talk = 106041, npc2 = 106041, emoji2 = 0, content = "思莉嘉？是你吗？", },


			{ npc1 = 106031, type = 2, emoji1 = 0, name = "思莉嘉", talk = 106031, npc2 = 106041, emoji2 = 0, content = "姑姑？是我姑姑！", },


			{ npc1 = 106031, type = 2, emoji1 = 0, name = "中年妇女", talk = 106041, npc2 = 106041, emoji2 = 0, content = "真的是你,我的孩子！你怎么到这里来了？他们是谁？", },


			{ npc1 = 106031, type = 2, emoji1 = 0, name = "思莉嘉", talk = 106031, npc2 = 106041, emoji2 = 0, content = "他们是我的朋友。", },


			{ npc1 = 106031, type = 1, emoji1 = 0, name = "爱拉忒", talk = 106021, npc2 = 106021, emoji2 = 0, content = "$playername,让她证明自己是这孩子的亲人,我们可不能把她交给不明不白的人。", },


			{ npc1 = 106041, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "请问这个孩子的家住在哪里？有什么亲人？", },


			{ npc1 = 106041, type = 1, emoji1 = 0, name = "中年妇女", talk = 106041, npc2 = 0, emoji2 = 0, content = "她住在王城外不远处的赫麦村。家里有父亲、母亲和爷爷。前一段时间,我接到他父亲的信,说村子里不太平静,也不知道现在怎么样了……", },

		},

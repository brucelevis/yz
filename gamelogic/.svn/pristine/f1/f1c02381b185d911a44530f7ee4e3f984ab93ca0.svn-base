			{ npc1 = 0, type = 4, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "对不起,我刚才没注意到,踩到了它……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


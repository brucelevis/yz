	[1121] = {
		texts = {
			
			{ npc1 = 112015, type = 1, emoji1 = 0, name = "赫塞尔", talk = 112015, npc2 = 0, emoji2 = 0, content = "哈哈哈,真是个有活力的年轻人啊！不禁让我回想起自己的少年时代,也是如此活泼,令人怀念啊！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 112015, type = 1, emoji1 = 0, name = "赫塞尔", talk = 112015, npc2 = 0, emoji2 = 0, content = "啊,对了,有什么事吗？看你的样子……是勇者大人吧！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 112015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "是的,布洛姆大人让我来……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 112015, type = 1, emoji1 = 0, name = "赫塞尔", talk = 112015, npc2 = 0, emoji2 = 0, content = "稍等。让我猜猜。你也是为了学习技能的事而来吧,哈哈哈看你钦佩的表情我就知道一定是猜对了！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 112015, type = 1, emoji1 = 0, name = "赫塞尔", talk = 112015, npc2 = 0, emoji2 = 0, content = "（介绍技能四）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 112015, type = 1, emoji1 = 0, name = "赫塞尔", talk = 112015, npc2 = 0, emoji2 = 0, content = "大概说清楚了吧……接下来就让我看看勇者大人听得算不算明白。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


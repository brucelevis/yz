	[51] = {
		texts = {
			
			{ npc1 = 5010, type = 1, emoji1 = 0, name = "米利暗", talk = 5010, npc2 = 0, emoji2 = 0, content = "果然很有天赋,不愧是吾王所罗门陛下的转世之人呐。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 5010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "被这样夸赞,真是叫人不好意思……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 5010, type = 1, emoji1 = 0, name = "米利暗", talk = 5010, npc2 = 0, emoji2 = 0, content = "想必在之前的切磋中你已注意到,对手的招式均有不同套路配以相应的武器了吧。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 5010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "没错。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 5010, type = 1, emoji1 = 0, name = "米利暗", talk = 5010, npc2 = 0, emoji2 = 0, content = "他们也是来自不同职业的勇者呢。所谓职业,就是勇者大人们在未来的冒险中不断努力的方向啊。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 5010, type = 1, emoji1 = 0, name = "米利暗", talk = 5010, npc2 = 0, emoji2 = 0, content = "不同职业赋予勇者在力量方面有所差异。在未来的团队协作中,勇者们也要依据职业特点发挥相应的作用啊。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 5010, type = 1, emoji1 = 0, name = "米利暗", talk = 5010, npc2 = 0, emoji2 = 0, content = "选择了适合自己的职业,勇者们也好,我们的迦南王国也好,未来都将是充满希望的。", option1 = "", option2 = "", option3 = "", option4 = "", },


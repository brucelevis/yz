	[1081] = {
		texts = {
			
			{ npc1 = 108010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 108020, emoji2 = 0, content = "“但他林”……魔神的名字吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 108020, npc2 = 108020, emoji2 = 0, content = "只是72位魔神中的一位而已。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 108020, npc2 = 108020, emoji2 = 0, content = "据说这个家伙诡怪异常,从不以真面目示人,狡猾得很呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 108020, emoji2 = 0, content = "那么,应该怎样处置这个人呢？？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "伊菲", talk = 108010, npc2 = 108020, emoji2 = 0, content = "惩罚什么的也不必太严厉啦,不过可别拦着我扒光他的裤子打屁股！呃……哈哈哈哈！开玩笑的。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 108020, npc2 = 108020, emoji2 = 0, content = "伊菲君的玩笑向来如此粗鄙吗？实在想这样做的话,请在我看不到的时候自便吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "？？？", talk = 0, npc2 = 108020, emoji2 = 0, content = "好啦好啦,烤兔子这样的美味最能消气了不是么？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 108020, npc2 = 108020, emoji2 = 0, content = "大家都快筋疲力尽了,$playername$sex:君|酱#却有时间烤兔子吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 108020, emoji2 = 0, content = "诶？刚才说话的,并不是我啊。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "伊菲", talk = 108010, npc2 = 108020, emoji2 = 0, content = "不是$playername$sex:君|酱#,难道是我说的？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "？？？", talk = 108030, npc2 = 108030, emoji2 = 0, content = "对啊,难道是我说的？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 108030, emoji2 = 0, content = "！！！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 108010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 108030, emoji2 = 0, content = "伊、伊菲君,怎么有两位？！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 6, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "（激烈的战斗过后……）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


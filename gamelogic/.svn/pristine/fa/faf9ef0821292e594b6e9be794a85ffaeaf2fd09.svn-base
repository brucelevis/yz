	[71] = {
		texts = {
			
			{ npc1 = 7010, type = 1, emoji1 = 0, name = "米利暗", talk = 7010, npc2 = 0, emoji2 = 0, content = "【职业】？的确很适合你。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 7010, type = 1, emoji1 = 0, name = "米利暗", talk = 7010, npc2 = 0, emoji2 = 0, content = "对于勇者来说,在冒险中不断提升自己的实力是非常必要的。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 7010, type = 1, emoji1 = 0, name = "米利暗", talk = 7010, npc2 = 0, emoji2 = 0, content = "不过出门在外,一己之力显然是不够。勇者的冒险从来不是某一个人的故事,尽快找到投契的队友,旅程会更加安全而有趣呢。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 7010, type = 1, emoji1 = 0, name = "米利暗", talk = 7010, npc2 = 0, emoji2 = 0, content = "不同职业的队友相互扶持,也可以有效提高冒险的效率。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 7010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "那么米利暗大人,怎样才能找到不同职业的队友呢？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 7010, type = 1, emoji1 = 0, name = "米利暗", talk = 7010, npc2 = 0, emoji2 = 0, content = "王国为了方便勇者们联络和互助,允许大家自行组建公会。在那里,你一定会找到志同道合的战友。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[72] = {
		texts = {
			
			{ npc1 = 7010, type = 1, emoji1 = 0, name = "米利暗", talk = 7010, npc2 = 0, emoji2 = 0, content = "越早加入公会,对勇者们的成长越有利。希望你能尽快找到适合自己的集体和伙伴。", option1 = "", option2 = "", option3 = "", option4 = "", },


	[1081] = {
		texts = {
			
			{ npc1 = 108015, type = 1, emoji1 = 0, name = "艾格娅", talk = 108015, npc2 = 0, emoji2 = 0, content = "果然还是勇者大人更胜一筹呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 108015, type = 1, emoji1 = 0, name = "艾格娅", talk = 108015, npc2 = 0, emoji2 = 0, content = "请即刻到下一位技能教官处吧,说起来,莱伦桑也曾是十分优秀的勇者呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


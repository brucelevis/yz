	[3102] = {
		texts = {
			
			{ npc1 = 310030, type = 1, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 0, emoji2 = 0, content = "啊……刚才对不住了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "你、你没事吧？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 1, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 0, emoji2 = 0, content = "很抱歉,想起了一些往事,一时有些无法自已了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 1, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 0, emoji2 = 0, content = "为什么不逃跑,我是魔神,独自面对这些事也是不会有危险的。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "说到底,是我害赛共大人想起往事,这样一走了之太不负责任了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 310020, emoji2 = 0, content = "赛共大人,您与华利弗,还有赫麦的村民,到底有怎样的恩怨呢？恕我多言,这似乎是一个很复杂的故事呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "是。华利弗曾诱使赫麦村人将我杀死,并向巴力献祭。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "这怎么可能？成为魔神之前,你的躯体就是凡铁不能伤害,邪术也无法侵犯的啊。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "没错,可是他们设计让我杀害了一个无罪的迦南人。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "我的身体是迦南人的祖先以黄金炼铸,灵魂也是他们虔诚祈求的结果。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "所以,在我杀害那个无罪之人后,身上的保护就消失了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "再后来,我死了,死在赫麦的迦南人手下。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "是华利弗诱惑了他们,赫麦人只是……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "他们选择了罪恶,华利弗只是成全了他们！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 310020, emoji2 = 0, content = "赛共大人……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "平凡的迦南人怎么可能对抗魔神呢？就算是赛共大人您,也不是华利弗的对手啊。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "几位又有谁是华利弗的对手吗？不是也勇敢地站出来了吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "伊菲", talk = 310010, npc2 = 310010, emoji2 = 0, content = "我们可是拯救迦南的勇者！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310010, emoji2 = 0, content = "没有人天生就是勇者！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "伊菲", talk = 310010, npc2 = 310010, emoji2 = 0, content = "可是……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "别说了。打扰这么久实在抱歉……或许我们该尽早想想别的办法。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "所以是打算放弃了吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "您说的对,没有人天生就是勇者。自己选择的道路,不应该怨怼他人。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "大人的行迹我们不会告诉其他人,这一点请放心。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "回赫麦村去看看吧,华利弗大概不会轻易放过他们呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "至于那个孩子,我会去试着救她,算是报答你们帮我击退心魔吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "赛共……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "不过希望你能明白,我救她,并非因为什么与能力相匹配的责任,而是出于自愿。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "爱拉忒吗,自大之人审视他人,强大之人审视自我,希望你以后可以成为一个真正强大的人。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "……我……明白了。谢谢你,赛共大人。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "还有一件事想请教大人：一直在这里,是等什么人吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "与人约定在此见面,可他一直没有出现呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 310020, emoji2 = 0, content = "是一个小男孩吗？他似乎也在等您呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "赛共", talk = 310030, npc2 = 310020, emoji2 = 0, content = "成为魔神之后,从前的很多事情都不记得了,实际上,我也不知道自己在等谁。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "即使忘记了也要坚持等待吗……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 310030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 310020, npc2 = 310020, emoji2 = 0, content = "那么,思莉嘉的事就拜托赛共大人啦！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },

		},
	},

	[4011] = {
		texts = {
			
			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "回到赫麦村时,那里与之前已是大不相同了……。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "阴霾笼罩着整个小村,没有一丝阳光", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "空中不时划过一道闪电,", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "远不足以劈开紧紧扼在心头的沉郁之感。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "伊菲", talk = 401010, npc2 = 401020, emoji2 = 0, content = "这是……之前的那个村子吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 401020, npc2 = 401020, emoji2 = 0, content = "怕了吗？接下来要面对的可是华利弗,心存恐惧的话,现在逃跑的话还来得及。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "伊菲", talk = 401010, npc2 = 401020, emoji2 = 0, content = "这么说就太瞧不起人了。临阵退缩可不是我的做派！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 401020, emoji2 = 0, content = "上次来的时候这里还有很多人,没想到不过月余光景……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 401020, emoji2 = 0, content = "他们不会已经遭了华利弗的毒手吧……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 401020, npc2 = 401020, emoji2 = 0, content = "如赛共说的,华利弗不会轻易放过他们的。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "伊菲", talk = 401010, npc2 = 401020, emoji2 = 0, content = "这里有古怪,大家小心！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 401020, emoji2 = 0, content = "发现了什么？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "伊菲", talk = 401010, npc2 = 401020, emoji2 = 0, content = "听！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 401010, type = 2, emoji1 = 0, name = "？？？", talk = 0, npc2 = 401020, emoji2 = 0, content = "科……科……科科……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },



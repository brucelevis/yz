	[1141] = {
		texts = {
			
			{ npc1 = 114015, type = 1, emoji1 = 0, name = "赫塞尔", talk = 114015, npc2 = 0, emoji2 = 0, content = "哈哈哈,有如此天赋,真是叫人羡慕呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 114015, type = 1, emoji1 = 0, name = "赫塞尔", talk = 114015, npc2 = 0, emoji2 = 0, content = "我有一个小礼物要送给你,不过在那之前,你可能需要去找一个叫做撒克里的老古板。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 114015, type = 1, emoji1 = 0, name = "赫塞尔", talk = 114015, npc2 = 0, emoji2 = 0, content = "为了防止这家伙刁难你,我为你准备了一封介绍信。他此时应该在圣殿,快去吧,否则好东西要被别人领走了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


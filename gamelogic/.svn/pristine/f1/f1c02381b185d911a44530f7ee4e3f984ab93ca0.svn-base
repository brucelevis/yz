	[2072] = {
		texts = {
			
			{ npc1 = 207020, type = 2, emoji1 = 0, name = "爱拉忒", talk = 207020, npc2 = 207030, emoji2 = 0, content = "思莉嘉,快过来！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207020, type = 2, emoji1 = 0, name = "思莉嘉", talk = 207030, npc2 = 207030, emoji2 = 0, content = "爷爷他……不会有事的吧？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207010, type = 2, emoji1 = 0, name = "伊菲", talk = 207010, npc2 = 207030, emoji2 = 0, content = "刚刚还要将别人灭口的禽兽一样的人,还担心他做什么？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207010, type = 2, emoji1 = 0, name = "思莉嘉", talk = 207030, npc2 = 207030, emoji2 = 0, content = "爷爷是我唯一个亲人了……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 207030, emoji2 = 0, content = "思莉嘉还这么小,除了爷爷还能依靠谁呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207010, type = 2, emoji1 = 0, name = "老人", talk = 207040, npc2 = 207040, emoji2 = 0, content = "唉……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 207030, npc2 = 207040, emoji2 = 0, content = "爷爷……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207030, type = 2, emoji1 = 0, name = "老人", talk = 207040, npc2 = 207040, emoji2 = 0, content = "别害怕。还要多谢几位救了我们祖孙……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 207040, emoji2 = 0, content = "抱歉,请问刚才发生的事,您还记得吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207030, type = 2, emoji1 = 0, name = "老人", talk = 207040, npc2 = 207040, emoji2 = 0, content = "这段时间的事情,全部都清清楚楚地记得啊。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207030, type = 2, emoji1 = 0, name = "老人", talk = 207040, npc2 = 207040, emoji2 = 0, content = "昏昏沉沉地从王城回来以后,我的身体就被一股邪恶的力量控制着。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207030, type = 2, emoji1 = 0, name = "老人", talk = 207040, npc2 = 207040, emoji2 = 0, content = "在它的驱使下,我为魔神建造祭坛,焚烧祭品,甚至杀害村民作为人牲,犯下了不可饶恕的错误。并非为了逃避责任,但我确实完全无法反抗。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 207040, emoji2 = 0, content = "邪恶的力量？那么,这股神秘的邪恶力量究竟要做什么呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207030, type = 2, emoji1 = 0, name = "老人", talk = 207040, npc2 = 207040, emoji2 = 0, content = "我只知道一个名字而已：巴力。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207020, type = 1, emoji1 = 0, name = "爱拉忒", talk = 207020, npc2 = 207040, emoji2 = 0, content = "巴力？！所罗门王手下的第一魔神？！可它不是……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207020, type = 1, emoji1 = 0, name = "老人", talk = 207040, npc2 = 207040, emoji2 = 0, content = "不错。据说巴力法力强悍无人能敌,即使是在所罗门陛下监管之下,心中也并未真正臣服啊。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207020, type = 1, emoji1 = 0, name = "老人", talk = 207040, npc2 = 207040, emoji2 = 0, content = "王上垂暮之年唯恐巴力失控为祸,设法将巴力肉身杀死,并分解镇压在不同的地方。如果让它复活,后果不堪设想。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207020, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 207040, emoji2 = 0, content = "如果是这样的话,幕后黑手是想借您之手复活巴力吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207020, type = 1, emoji1 = 0, name = "爱拉忒", talk = 207020, npc2 = 207040, emoji2 = 0, content = "其余七十一位魔神中,或是尊崇或是畏惧巴力的,有不少成了它的追随者。现在它们逃逸在外,企图复活巴力也不是不可能。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207010, type = 2, emoji1 = 0, name = "伊菲", talk = 207010, npc2 = 207020, emoji2 = 0, content = "等等……似乎有奇怪的声音,听到了吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },

		},
	},


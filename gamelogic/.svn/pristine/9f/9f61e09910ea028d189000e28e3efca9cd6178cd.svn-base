			{ npc1 = 110015, type = 1, emoji1 = 0, name = "布洛姆", talk = 110015, npc2 = 0, emoji2 = 0, content = "哼,勇者吗？是想问战斗的技巧吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },



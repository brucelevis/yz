			{ npc1 = 210050, type = 1, emoji1 = 0, name = "黑雾", talk = 210050, npc2 = 0, emoji2 = 0, content = "有点意思,还会再见面的,诸位,后会有期。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210010, type = 1, emoji1 = 0, name = "伊菲", talk = 210010, npc2 = 0, emoji2 = 0, content = "想跑？哪有那么容易？！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },



	[2022] = {
		texts = {
			
			{ npc1 = 202030, type = 1, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 0, emoji2 = 0, content = "谢……谢谢……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "这匹狼眸色血红体型硕大,好像不是一般的野兽呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 1, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 0, emoji2 = 0, content = "桑、桑卡……是桑卡吗！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "诶？是认识的吗？这样的话……为什么会被袭击呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 1, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 0, emoji2 = 0, content = "不知道呢……桑卡是爷爷驯养的猎狼,平时很温驯的。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 1, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 0, emoji2 = 0, content = "自从上次从王城回来,爷爷就变得很奇怪,现在连桑卡也……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 202020, npc2 = 202020, emoji2 = 0, content = "能不能说说究竟是怎么回事呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 202020, emoji2 = 0, content = "我们一家是赫麦的村民,长辈们平时以打猎为生。因为猎物减少,生活越来越贫穷的我们一家人,在大约一个月前到王都贩卖以前囤积的兽皮。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 202020, emoji2 = 0, content = "就在那天深夜,圣殿的方向突然有一束光冲向夜空,街道上也被照得亮如白昼,随后我们听到一声巨响。之后爷爷就陷入了昏迷。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 202020, npc2 = 202020, emoji2 = 0, content = "一个月前,是圣约柜失踪,魔神出逃的那天吧……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "伊菲", talk = 202010, npc2 = 202010, emoji2 = 0, content = "爷爷是从那时候开始改变的吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 202010, emoji2 = 0, content = "是……爷爷昏迷了许多天,醒来后失去了部分记忆,连性情也变得暴躁了呢,因此村里的人都不太愿意接触他呢……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 202010, emoji2 = 0, content = "就是从那时起,村里的牲畜经常消失,不久后这座山被发现有大量用于献祭的畜类骨骸。再后来,孩子们也……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 202020, npc2 = 202020, emoji2 = 0, content = "难道,这些都是你的爷爷做的？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 202020, emoji2 = 0, content = "对不起……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 202020, npc2 = 202020, emoji2 = 0, content = "又是怎么发现的呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 202020, emoji2 = 0, content = "给爷爷送晚饭的时候,发现房间里没有人,思莉嘉就到外面找,然后跟着他进了山里。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 202020, emoji2 = 0, content = "就在那个被发现的祭坛旁,有一间地窖,里面关着失踪的孩子们。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 202030, npc2 = 202020, emoji2 = 0, content = "是我太不小心被发现了,爸爸妈妈是为救我相继去世的……争斗的声音惊动了村民们,那一次死伤了很多人,连村长爷爷也倒下了……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "伊菲", talk = 202010, npc2 = 202010, emoji2 = 0, content = "村长？赫麦村的村长吗？！这怎么可能？我们到这里来的时候赫麦村还是好好的啊！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 202030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 202010, emoji2 = 0, content = "看来非走一趟不可了呢。思莉嘉,请在前面带路吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },

		},
	},


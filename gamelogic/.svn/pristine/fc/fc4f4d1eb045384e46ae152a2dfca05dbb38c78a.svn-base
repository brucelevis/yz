gm = require "gamelogic.gm.init"

--- 用法: mergeserver 数据库ip 数据库端口 数据库名/编号 合并到的数据库ip 合并到的数据库端口 合并到的数据库名/编号
function gm.mergeserver(args)
	local isok,args = checkargs("string","int","int","string","int","int")
	if not isok then
		net.msg.S2C.notify(master_pid,"用法: mergeserver 数据库ip 数据库端口 数据库名/编号 合并到的数据库ip 合并到的数据库端口 合并到的数据库名/编号")
		return
	end
end

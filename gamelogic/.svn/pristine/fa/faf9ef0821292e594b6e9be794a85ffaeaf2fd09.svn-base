			{ npc1 = 201010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 201020, npc2 = 201020, emoji2 = 0, content = "好奇怪,在做什么啊？", option1 = "", option2 = "", option3 = "", option4 = "", },



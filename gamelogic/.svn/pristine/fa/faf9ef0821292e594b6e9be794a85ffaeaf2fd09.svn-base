			{ npc1 = 3010, type = 4, emoji1 = 0, name = "艾格娅", talk = 3010, npc2 = 0, emoji2 = 0, content = "非常好,接下来等待你们的,将是更强悍的对手。", option1 = "", option2 = "", option3 = "", option4 = "", },


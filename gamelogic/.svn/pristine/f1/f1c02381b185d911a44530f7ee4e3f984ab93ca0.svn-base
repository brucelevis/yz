			{ npc1 = 207010, type = 1, emoji1 = 0, name = "伊菲", talk = 207010, npc2 = 0, emoji2 = 0, content = "喂,有什么诡计,该告诉我们了吧？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207011, type = 2, emoji1 = 0, name = "思莉嘉", talk = 207030, npc2 = 207030, emoji2 = 0, content = "我不知道……我……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207020, type = 2, emoji1 = 0, name = "爱拉忒", talk = 207020, npc2 = 207030, emoji2 = 0, content = "你的爷爷到底在哪里？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207020, type = 2, emoji1 = 0, name = "思莉嘉", talk = 207030, npc2 = 207030, emoji2 = 0, content = "爷爷……爷爷他……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207040, type = 1, emoji1 = 0, name = "？？？", talk = 207040, npc2 = 0, emoji2 = 0, content = "思莉嘉,好孩子,带了重要的客人来吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207020, type = 1, emoji1 = 0, name = "爱拉忒", talk = 207020, npc2 = 0, emoji2 = 0, content = "小心！这个人身上魔气极重！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207040, type = 2, emoji1 = 0, name = "？？？", talk = 207040, npc2 = 207030, emoji2 = 0, content = "来吧我的孩子,到爷爷身边来。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 207040, type = 2, emoji1 = 0, name = "思莉嘉", talk = 207030, npc2 = 207030, emoji2 = 0, content = "不,不要……救救我,求求你们救救我！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },



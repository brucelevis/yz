			{ npc1 = 201040, type = 1, emoji1 = 0, name = "犬妖", talk = 201040, npc2 = 0, emoji2 = 0, content = "嗷——", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },



	[2092] = {
		texts = {
			
			{ npc1 = 0, type = 4, emoji1 = 0, name = "爱拉忒", talk = 209020, npc2 = 0, emoji2 = 0, content = "用尽全力了也不过如此吗？但他林大人？", option1 = "", option2 = "", option3 = "", option4 = "", },


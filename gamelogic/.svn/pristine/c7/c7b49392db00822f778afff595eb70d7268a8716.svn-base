	[1062] = {
		texts = {
			
			{ npc1 = 106031, type = 2, emoji1 = 0, name = "思莉嘉", talk = 106031, npc2 = 106041, emoji2 = 0, content = "哥哥姐姐,她真的是我姑姑。", },


			{ npc1 = 106011, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 106021, emoji2 = 0, content = "既然这样,那思莉嘉就拜托您照看了。", },



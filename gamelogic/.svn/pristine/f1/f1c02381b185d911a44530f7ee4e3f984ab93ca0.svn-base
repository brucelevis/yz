	[3041] = {
		texts = {
			
			{ npc1 = 304010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 304020, npc2 = 304020, emoji2 = 0, content = "应该是这里没错。传说赛共经常隐匿在此,魔神被封印之后,还有人将此地当做遗迹游览呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 304010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 304020, npc2 = 304020, emoji2 = 0, content = "不过现在应该已经没什么人到这里来了吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 304010, type = 2, emoji1 = 0, name = "伊菲", talk = 304010, npc2 = 304020, emoji2 = 0, content = "这里这么冷僻,怎么可能有人找得到啊？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },



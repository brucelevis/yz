data_1500_BranchTaskText = {

	[1011] = {
		texts = {
			
			{ npc1 = 101011, type = 1, emoji1 = 0, name = "伊菲", talk = 101011, npc2 = 0, emoji2 = 0, content = "还是王城好,繁华,兴旺,应有尽有。", },


			{ npc1 = 101011, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "不过到了这里可就不能像在山林旷野一样放肆了。迦南正在多事之秋,王城守备森严,小心惹来事端。、", },


			{ npc1 = 101041, type = 2, emoji1 = 0, name = "？？？", talk = 101031, npc2 = 101031, emoji2 = 0, content = "对不起,饶了我这次吧,我真的没有钱。嘤嘤嘤……", },


			{ npc1 = 101041, type = 2, emoji1 = 0, name = "恶霸", talk = 101041, npc2 = 101031, emoji2 = 0, content = "没钱？没钱谁准你在城里乱逛的？", },


			{ npc1 = 101041, type = 2, emoji1 = 0, name = "？？？", talk = 101031, npc2 = 101031, emoji2 = 0, content = "请你们再饶恕我一次吧……", },


			{ npc1 = 101011, type = 2, emoji1 = 0, name = "伊菲", talk = 101011, npc2 = 101021, emoji2 = 0, content = "青天白日欺凌弱小？！", },


			{ npc1 = 101011, type = 2, emoji1 = 0, name = "爱拉忒", talk = 101021, npc2 = 101021, emoji2 = 0, content = "听起来还是个小孩子。", },


			{ npc1 = 101011, type = 2, emoji1 = 0, name = "伊菲", talk = 101011, npc2 = 101021, emoji2 = 0, content = "哼,看我去教训他们！", },


			{ npc1 = 101011, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 101021, emoji2 = 0, content = "哎！不要冲动！", },

		},
	},

	[1012] = {
		texts = {
			
			{ npc1 = 0, type = 4, emoji1 = 0, name = "恶霸", talk = 101041, npc2 = 0, emoji2 = 0, content = "唉哟……勇者大人饶命啊！", },


			{ npc1 = 0, type = 4, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "要向你们的嘉卡大人告状尽管去,我可不怕他！", },


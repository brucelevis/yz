	[1021] = {
		texts = {
			
			{ npc1 = 102021, type = 2, emoji1 = 0, name = "爱拉忒", talk = 102021, npc2 = 102031, emoji2 = 0, content = "这孩子不是思莉嘉吗？", },


			{ npc1 = 102021, type = 2, emoji1 = 0, name = "思莉嘉", talk = 102031, npc2 = 102031, emoji2 = 0, content = "嘤……哥哥姐姐,是你们？", },


			{ npc1 = 102021, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 102031, emoji2 = 0, content = "你怎么在这儿？你的爷爷呢？", },


			{ npc1 = 102021, type = 2, emoji1 = 0, name = "思莉嘉", talk = 102031, npc2 = 102031, emoji2 = 0, content = "你们离开不久,赛共大人就为我解除了身上的魔法。爷爷说,不能回村子了,要带我投靠住在王城的姑姑。可是我们刚一下山,爷爷就得了重病,很快就去世了。", },


			{ npc1 = 102021, type = 2, emoji1 = 0, name = "爱拉忒", talk = 102021, npc2 = 102031, emoji2 = 0, content = "老人家长时间受华利弗魔气的侵害,身体状况实在……思莉嘉你又是怎么遇到这群人的？", },


			{ npc1 = 102021, type = 2, emoji1 = 0, name = "思莉嘉", talk = 102031, npc2 = 102031, emoji2 = 0, content = "我……我也不清楚。王城太大了,我找不到姑姑,昨天撞见了这群人,他们朝我要钱。说没有钱就像过路人讨……", },


			{ npc1 = 102021, type = 2, emoji1 = 0, name = "思莉嘉", talk = 102031, npc2 = 102031, emoji2 = 0, content = "如果今天还拿不出钱就把我关起来。", },


			{ npc1 = 102021, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 102031, emoji2 = 0, content = "嘉卡真是想钱想疯了,这种事也能纵容手下做得出来！", },


			{ npc1 = 102021, type = 2, emoji1 = 0, name = "思莉嘉", talk = 102031, npc2 = 102031, emoji2 = 0, content = "哥哥姐姐,我已经很久没吃饱饭了,你们有吃的吗？", },


			{ npc1 = 102021, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 102031, emoji2 = 0, content = "你等着。", },

		},
	},

	[1022] = {
		texts = {
			
			{ npc1 = 101003, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "请问有人在吗？", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "没有人我是死的吗？", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "老板好,脾气还是这么火爆啊,还记得我吗？", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "记得记得！你就是那个帮我教训了那群混蛋的勇者吧！", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "怎么,又来讨牛肉吗？", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "我这次可是会给钱的。", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "哈哈哈哈！那就多谢啦！", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "这么久不见,你到那里施展拳脚去了？", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "这话可就长了。", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "啊那算了！我可没时间听你絮叨,今天要点什么啊？", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "还是嫩牛肉吧,一份熟的。", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "一份？太少了吧？您可是威风赫赫的勇者大人啊！", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "呃……那两份？", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "五份！", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "好……好多……", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "怎么样,够爽快吧！", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "好！就五份。", },


			{ npc1 = 101003, type = 1, emoji1 = 0, name = "肉铺老板", talk = 101003, npc2 = 0, emoji2 = 0, content = "银货两讫,您的牛肉,勇者大人！", },

		},
	},

	[1032] = {
		texts = {
			
			{ npc1 = 103011, type = 2, emoji1 = 0, name = "伊菲", talk = 103011, npc2 = 103021, emoji2 = 0, content = "好蠢！被人骗了都不知道。", },


			{ npc1 = 103011, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 103021, emoji2 = 0, content = "被骗？", },


			{ npc1 = 103011, type = 2, emoji1 = 0, name = "爱拉忒", talk = 103021, npc2 = 103021, emoji2 = 0, content = "人家三言两语就让你多买了四份牛肉。你吃得了那么多？天这么热,很快就会坏掉的。", },


			{ npc1 = 103011, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 103021, emoji2 = 0, content = "那思莉嘉多吃点。", },


			{ npc1 = 103011, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 103021, emoji2 = 0, content = "我们不能照顾她太久,还是尽快找到他的家人比较好。", },


			{ npc1 = 104011, type = 2, emoji1 = 0, name = "爱拉忒", talk = 104021, npc2 = 104021, emoji2 = 0, content = "那就开始行动吧！", },


			{ npc1 = 104011, type = 2, emoji1 = 0, name = "伊菲", talk = 104011, npc2 = 104021, emoji2 = 0, content = "我们分头进行吧,这样快一些。", },


			{ npc1 = 104011, type = 2, emoji1 = 0, name = "爱拉忒", talk = 104021, npc2 = 104021, emoji2 = 0, content = "王城这么大,要找到什么时候？我们把消息写在布告牌上,思莉嘉的姑姑看到了,就回来找我们。", },


			{ npc1 = 104011, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 104021, emoji2 = 0, content = "这个主意不错,就这么办！", },

		},

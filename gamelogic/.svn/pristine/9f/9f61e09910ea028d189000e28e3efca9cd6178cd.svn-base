			{ npc1 = 114025, type = 1, emoji1 = 0, name = "撒克里", talk = 114025, npc2 = 0, emoji2 = 0, content = "已经有一百三十多位勇者了,这样的异象真的没有问题吗……？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 114025, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "撒克里大人？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 114025, type = 1, emoji1 = 0, name = "撒克里", talk = 114025, npc2 = 0, emoji2 = 0, content = "呃……抱歉。最近王国出现的变故实在太多了。身为大祭司大人首席护法的我不得不谨慎一些。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 114025, type = 1, emoji1 = 0, name = "撒克里", talk = 114025, npc2 = 0, emoji2 = 0, content = "虽然已经取得了其他四位护法的认可,还是不得不过我这一关才行。这是出于保护王国和勇者的考虑。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 114025, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "请撒克里大人放心的考验我吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },



	[3062] = {
		texts = {
			
			{ npc1 = 0, type = 4, emoji1 = 0, name = "赛共", talk = 306030, npc2 = 0, emoji2 = 0, content = "你们很强,不过我还是不会答应你们的要求,别再浪费时间了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


	[62] = {
		texts = {
			
			{ npc1 = 6010, type = 1, emoji1 = 0, name = "登记官", talk = 6010, npc2 = 0, emoji2 = 0, content = "“$playername”（君或酱）吗？这是你的【职业】证。协会内暂无紧要的任务,你可以回到圣殿向大祭司大人询问接下来的历练安排。", option1 = "", option2 = "", option3 = "", option4 = "", },


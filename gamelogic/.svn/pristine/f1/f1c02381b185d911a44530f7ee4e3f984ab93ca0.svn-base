	[3051] = {
		texts = {
			
			{ npc1 = 305020, type = 2, emoji1 = 0, name = "爱拉忒", talk = 305020, npc2 = 305030, emoji2 = 0, content = "赛共……是赛共大人！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 305020, type = 2, emoji1 = 0, name = "赛共", talk = 305030, npc2 = 305030, emoji2 = 0, content = "时隔这么多年,没想到这个名字还有人记得。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 305020, type = 2, emoji1 = 0, name = "爱拉忒", talk = 305020, npc2 = 305030, emoji2 = 0, content = "赛共大人当然值得人们铭记啊！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 305020, type = 2, emoji1 = 0, name = "赛共", talk = 305030, npc2 = 305030, emoji2 = 0, content = "哈哈哈哈,这种漂亮话还是说给别人听吧。有什么事吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 305020, type = 2, emoji1 = 0, name = "爱拉忒", talk = 305020, npc2 = 305030, emoji2 = 0, content = "赛共大人,请救救我的朋友思莉嘉吧,她是赫麦村人,迦南王国的子民。不幸被华利弗烙上了契印,至今还未苏醒。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 305020, type = 2, emoji1 = 0, name = "赛共", talk = 305030, npc2 = 305030, emoji2 = 0, content = "你是说赫麦村的人？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 305020, type = 2, emoji1 = 0, name = "赛共", talk = 305030, npc2 = 305030, emoji2 = 0, content = "难道你不清楚,这是我最大的忌讳吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 305010, type = 2, emoji1 = 0, name = "伊菲", talk = 305010, npc2 = 305020, emoji2 = 0, content = "喂喂！一言不合就开战啊？早就劝过爱拉忒酱不要自讨没趣的,现在怎么收场啦！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 6, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "（激烈的战斗过后……）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


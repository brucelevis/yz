data_1500_ZhiyinTaskText = {

	[1011] = {
		texts = {
			
			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "永夜即将降临,圣怒何以平息", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "无所不能的先王所罗门陛下\n愿您从异世无尽的长眠中苏醒", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "带领您的子民,开辟希望与光明的出路", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "这里是什么地方……刚才……我……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "由传送之门而来的异界勇者吗？不错,很有活力的年轻人啊。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "我是这里的大祭司米利暗,有什么问题的话,阁下可以尽管问我。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "对不起,请问……这到底是怎么一回事？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "唉……这件事实在不宜声张,不过作为肩负着拯救迦南的使命的勇者,知道这些也是必须的。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "在如你所见的这座迦南的圣殿之中,原有两件令人忌惮的相克宝物。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "其一是我们的先祖与神定下的契约,其二是四百余年前,先圣王所罗门陛下降服并封印的魔神。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "魔神？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "是的,这些魔神,有七十二位之多。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "如果这个平衡没有被打破,迦南王国也许会一直平静下去。然而近日存放契约的圣约柜竟离奇消失。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "失去了制约的魔神们很快突破了封印,逃亡各地作乱,以致民不聊生。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "原来是这样啊……那么,我又是怎么到这里的呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "圣王所罗门陛下曾有遗训：于迦南危急时刻开启传送之门,陛下将重现在这片土地上,带领我们开辟希望之路。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },

		},
	},

	[1021] = {
		texts = {
			
			{ npc1 = 102015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "愿您从异世无尽的长眠中……苏醒……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "带领子民,开辟希望与光明的出路。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "那个人……祭司大人就是您吧？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "请叫我米利暗吧。虽然是大祭司,也不能承受被勇者们称作“大人”呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "那么,圣谕祭司说的所罗门王的转世“之一”又是怎样一回事呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "在传送之门开启的瞬间,圣殿里出现了数百位勇者。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "很抱歉,如此混乱的原因尚不明确,不过或许大家都分别承担了所罗门陛下的力量也说不定。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "不过,如果是得遇机缘的勇者大人的话,一定会找出导致这个情况的原因吧！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "迦南勇者", talk = 0, npc2 = 0, emoji2 = 0, content = "可是……就算是我这样从未接受过训练的人,也能成为一名合格的勇者吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "别担心,所谓勇者自然离不开冒险与战斗,通过不断地努力,一点一点强大起来。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "这样的改变可不是一朝一夕完成的。勇者大人的话,就算是我也完全找不出不信任的理由呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "不过收复魔神也好,寻找圣约柜也好,都是十分危险的。有了精良的护具和武器才能事半功倍啊。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "王国已经为勇者们准备了基本的装备,请穿上之后仔细体验它的力量吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },

		},
	},

	[1032] = {
		texts = {
			
			{ npc1 = 103015, type = 1, emoji1 = 0, name = "米利暗", talk = 103015, npc2 = 0, emoji2 = 0, content = "非常好。不过仅仅如此远远不够,与敌人格斗竞技,技巧也是非常关键的。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 103015, type = 1, emoji1 = 0, name = "米利暗", talk = 103015, npc2 = 0, emoji2 = 0, content = "我的护法们是这方面的好手,他们负责分别传授勇者们不同的战斗技能,你可以首先去找艾格娅学习。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 103015, type = 1, emoji1 = 0, name = "米利暗", talk = 103015, npc2 = 0, emoji2 = 0, content = "她就在圣殿外面,祝你好运。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },

		},
	},

	[1042] = {
		texts = {
			
			{ npc1 = 104015, type = 1, emoji1 = 0, name = "艾格娅", talk = 104015, npc2 = 0, emoji2 = 0, content = "原来是从传送之门而来的勇者大人,真是失礼了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 104015, type = 1, emoji1 = 0, name = "艾格娅", talk = 104015, npc2 = 0, emoji2 = 0, content = "既然是大祭司大人青睐的勇者,一定会与众不同呢。不过还是要检验一下你的能力才行。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


			{ npc1 = 104015, type = 1, emoji1 = 0, name = "艾格娅", talk = 104015, npc2 = 0, emoji2 = 0, content = "没有相当实力的话,即使是在学习和训练当中,受伤也是很有可能发生的。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 0, },


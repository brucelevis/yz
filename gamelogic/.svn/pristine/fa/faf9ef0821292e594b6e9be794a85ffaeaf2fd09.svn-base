	[61] = {
		texts = {
			
			{ npc1 = 6010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "请问加入【职业】是在这里登记吗？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 6010, type = 1, emoji1 = 0, name = "登记官", talk = 6010, npc2 = 0, emoji2 = 0, content = "是呢。看来是一位非常优秀的勇者啊。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 6010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "能得到您的认可,真是不胜感激。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 6010, type = 1, emoji1 = 0, name = "登记官", talk = 6010, npc2 = 0, emoji2 = 0, content = "将你的名字写在这里,【职业】的荣耀将与你同在！", option1 = "", option2 = "", option3 = "", option4 = "", },


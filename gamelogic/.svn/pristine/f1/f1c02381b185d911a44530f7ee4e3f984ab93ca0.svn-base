			{ npc1 = 210020, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "晕过去了吗……？得想办法救醒她。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210020, type = 1, emoji1 = 0, name = "爱拉忒", talk = 210020, npc2 = 0, emoji2 = 0, content = "奇怪,她的颈上似乎多了一个印记……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210010, type = 2, emoji1 = 0, name = "伊菲", talk = 210010, npc2 = 210020, emoji2 = 0, content = "可恶！被它跑了,还卷走了我一支箭。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "老人", talk = 210040, npc2 = 210020, emoji2 = 0, content = "这是……这不是魔神华利弗的印记吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210020, type = 2, emoji1 = 0, name = "爱拉忒", talk = 210020, npc2 = 210020, emoji2 = 0, content = "华利弗？寻常的村民也知道魔神华利弗吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "老人", talk = 210040, npc2 = 210020, emoji2 = 0, content = "哎……赫麦村一向以强盗之村著称,其实,这一切都是有原因的。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "老人", talk = 210040, npc2 = 210020, emoji2 = 0, content = "这村子原本是一个籍籍无名的小地方,数百年前,所罗门王还未收降魔神时,华利弗以此地为根基,大肆怂恿人们盗窃犯罪。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "老人", talk = 210040, npc2 = 210020, emoji2 = 0, content = "赫麦村民因此一夜暴富,人们得到华利弗的指引,所以尊他为神,向其献祭、祝祷。后世的人供奉这位魔神,并保留了偷盗的陋俗。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "老人", talk = 210040, npc2 = 210020, emoji2 = 0, content = "没想到,华利弗从圣殿逃脱之后,居然最先将魔爪伸向了这里。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "爱拉忒", talk = 210020, npc2 = 210020, emoji2 = 0, content = "信仰罪恶,亦将被罪恶反噬。与魔神为伍,自然首当其冲遭受荼毒。话虽不好听,却也是赫麦人咎由自取。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 210020, emoji2 = 0, content = "听思莉嘉说,老先生以打猎为生,是后来搬到赫麦村的吧？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "老人", talk = 210040, npc2 = 210020, emoji2 = 0, content = "不。我从小就在这里。不过十几年前曾遇到过一位很厉害的游侠,学了几手本事,洗心革面啦。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "老人", talk = 210040, npc2 = 210020, emoji2 = 0, content = "不过,还是连累了这个孩子。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210040, type = 2, emoji1 = 0, name = "爱拉忒", talk = 210020, npc2 = 210020, emoji2 = 0, content = "思莉嘉的事,我们会尽力帮忙。这段时间还请您妥善照顾她。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210010, type = 2, emoji1 = 0, name = "伊菲", talk = 210010, npc2 = 210020, emoji2 = 0, content = "不是吧！这样下去什么时候才能回到王城啊？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 210020, npc2 = 210020, emoji2 = 0, content = "不想帮忙的话,伊菲君请先回去吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 210010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 210020, emoji2 = 0, content = "事不宜迟,现在就出发吧！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },

		},
	},

	[3011] = {
		texts = {
			
			{ npc1 = 301010, type = 1, emoji1 = 0, name = "伊菲", talk = 301010, npc2 = 0, emoji2 = 0, content = "现在要去哪里呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 301010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 301020, npc2 = 301020, emoji2 = 0, content = "当务之急难道不是寻找唤醒思莉嘉的方法吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 301010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 301020, npc2 = 301020, emoji2 = 0, content = "没猜错的话,那个印记似乎正是施法者用于控制他人而缔结的契印吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 301010, type = 2, emoji1 = 0, name = "伊菲", talk = 301010, npc2 = 301020, emoji2 = 0, content = "这么说的话,那个孩子现在可能还在魔神的掌控之中喽？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 301010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 301020, npc2 = 301020, emoji2 = 0, content = "不过请放心吧。传说中魔神赛共大人就在这座山里。找到它的话,也许有办法解除思莉嘉的契印呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 301010, type = 2, emoji1 = 0, name = "伊菲", talk = 301010, npc2 = 301020, emoji2 = 0, content = "可是爱拉忒酱不是说过,“与魔神为伍,必然被受到荼毒”的话吗？爱拉忒酱要想清楚啊！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 301010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 301020, npc2 = 301020, emoji2 = 0, content = "赛共大人与其他魔神可是完全不同的！不试试怎么知道行不行呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 301010, type = 2, emoji1 = 0, name = "伊菲", talk = 301010, npc2 = 301020, emoji2 = 0, content = "拜托,这样自作主张真的不要紧吗？！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 301010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 301020, emoji2 = 0, content = "真是头疼……请不要吵了两位。这山里怪物很多,大家警惕一点吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 6, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "（激烈的战斗过后……）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


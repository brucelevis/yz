	[41] = {
		texts = {
			
			{ npc1 = 4010, type = 1, emoji1 = 0, name = "艾格娅", talk = 4010, npc2 = 0, emoji2 = 0, content = "只靠粗拳笨腿在战斗中是无法取胜的,学习并掌握技能也是至关重要的事。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 4010, type = 1, emoji1 = 0, name = "艾格娅", talk = 4010, npc2 = 0, emoji2 = 0, content = "已经掌握了这一诀窍的话,就请迎接新的挑战吧。", option1 = "", option2 = "", option3 = "", option4 = "", },



	[1052] = {
		texts = {
			
			{ npc1 = 105041, type = 1, emoji1 = 0, name = "嘉卡", talk = 105041, npc2 = 0, emoji2 = 0, content = "你……你……！", },


			{ npc1 = 105041, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "放心,我还没野蛮到随意伤害他人性命的地步,只是稍微教训你一下而已。", },


			{ npc1 = 105041, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "但是,如果再让我看到阁下欺凌弱小,可就不是一顿拳头这么便宜了。", },


			{ npc1 = 105041, type = 1, emoji1 = 0, name = "嘉卡", talk = 105041, npc2 = 0, emoji2 = 0, content = "我……我要告诉睦旦大人去！你们竟然敢如此无法无天！咱们走着瞧！", },


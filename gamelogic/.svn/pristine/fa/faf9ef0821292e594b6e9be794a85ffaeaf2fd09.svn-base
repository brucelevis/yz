	[81] = {
		texts = {
			
			{ npc1 = 8010, type = 1, emoji1 = 0, name = "米利暗", talk = 8010, npc2 = 0, emoji2 = 0, content = "当然,在旅程中结识朋友也是很不错的选择。并肩作战的友谊可是最难能可贵的。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 8010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "嗯！可是……现在应该如何继续提升自己呢？对这里完全不熟悉的我,似乎很难……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 8010, type = 1, emoji1 = 0, name = "米利暗", talk = 8010, npc2 = 0, emoji2 = 0, content = "正因为陌生,所以才叫冒险的呀。勇者们的修炼可不是照本宣科一成不变的。不过唯有经历了真正令人血脉贲张的激烈的战斗,才能领悟“勇者”这个光荣的称谓意义之所在吧。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[82] = {
		texts = {
			
			{ npc1 = 8010, type = 1, emoji1 = 0, name = "米利暗", talk = 8010, npc2 = 0, emoji2 = 0, content = "王城附近的赫麦村最近似乎有些不平静,$playername,请到那里去看看吧。赫麦虽是村镇,但与王城相邻。保卫迦南的重任,就交给$playername（君或酱）啦！", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 8010, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "有了米利暗大人的指导,似乎安心了许多呢。也请大人将迦南的安危放心地交给我吧！", option1 = "", option2 = "", option3 = "", option4 = "", },

		},

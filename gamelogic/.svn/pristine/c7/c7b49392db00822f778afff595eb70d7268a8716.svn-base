	[1042] = {
		texts = {
			
			{ npc1 = 0, type = 3, emoji1 = 0, name = "nil", talk = 0, npc2 = 0, emoji2 = 0, content = "时间匆匆流逝,夕阳西下。", },


			{ npc1 = 104011, type = 2, emoji1 = 0, name = "伊菲", talk = 104011, npc2 = 104021, emoji2 = 0, content = "这么久也没有人……不过思莉嘉不要担心,你姑姑一定会看到的。", },



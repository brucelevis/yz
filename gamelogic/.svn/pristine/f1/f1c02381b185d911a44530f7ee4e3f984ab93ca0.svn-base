	[1101] = {
		texts = {
			
			{ npc1 = 110030, type = 1, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 0, emoji2 = 0, content = "四百年了,已经四百年没有经历这样令人血脉贲张的战斗了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 0, emoji2 = 0, content = "你们激起了我对鲜血的渴望,对征战的热情。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110010, type = 1, emoji1 = 0, name = "伊菲", talk = 110010, npc2 = 0, emoji2 = 0, content = "自言自语的,在说什么啊？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 0, emoji2 = 0, content = "不错,非常不错,尤其是你小姑娘。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 110020, npc2 = 110020, emoji2 = 0, content = "哼！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 110020, emoji2 = 0, content = "不甘、愤怒让你与你的前辈们一样,渴望强大,渴望被认同。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 110020, npc2 = 110020, emoji2 = 0, content = "闭嘴,你这个恶魔！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 110020, emoji2 = 0, content = "做我的使魔吧？假以时日,一定会成为非常优秀的杀人利器。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 110020, emoji2 = 0, content = "你想得到的,都会成为你的囊中之物,你想毁灭的,必将一一化为齑粉。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 110020, npc2 = 110020, emoji2 = 0, content = "住口！你给我住口！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 110020, emoji2 = 0, content = "冷静,爱拉忒酱！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 110020, emoji2 = 0, content = "害怕什么？害怕你那脆弱的小心肝,关不住巫毒的叛逆和沸腾的杀意？或者,我该叫你——法师？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 110020, emoji2 = 0, content = "法师？巫毒……和法师有什么关系？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "伊菲", talk = 0, npc2 = 110020, emoji2 = 0, content = "现在不是说这个的时候吧！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 110020, emoji2 = 0, content = "抛弃它吧,释放你的本性吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 110020, npc2 = 110020, emoji2 = 0, content = "口口声声说着恶毒的话,到底对法师了解多少？这样挑拨人心煽动罪恶的魔神究竟对生为法师的人们了解多少呢？！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "伊菲", talk = 110010, npc2 = 110010, emoji2 = 0, content = "爱拉忒酱,别被它骗了！喂,怪物,快点滚回圣殿去,乖乖看守你的魔柱吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 110010, emoji2 = 0, content = "哼,小东西,再过一百年,你这番话我还会考虑一下。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 110010, emoji2 = 0, content = "但是现在,还没有这个资本。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 110020, npc2 = 110020, emoji2 = 0, content = "够了,这种没有感情,只会玩弄他人的魔鬼,根本不值得我们多费唇舌。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 110020, npc2 = 110020, emoji2 = 0, content = "叛逆与杀意,有过真的存在,此时此刻我最想杀死的,就是恶意曲解魔法师的混蛋！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 110020, npc2 = 110020, emoji2 = 0, content = "想所罗门陛下忏悔吧,恶毒的魔神！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 2, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 110020, emoji2 = 0, content = "小小巫毒也妄想成为审判魔神的人,不自量力。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 6, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "（激烈的战斗过后……）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },

		},
	},

	[1102] = {
		texts = {
			
			{ npc1 = 110010, type = 2, emoji1 = 0, name = "伊菲", talk = 110010, npc2 = 110020, emoji2 = 0, content = "爱拉忒酱,$playername$sex:君|酱#都还好吗？这边快撑……撑不住了……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 110020, npc2 = 110020, emoji2 = 0, content = "魔神比想象中的还难对付！是在顶不住的话,只有撤离了", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 110020, emoji2 = 0, content = "可是身为勇者……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110010, type = 2, emoji1 = 0, name = "伊菲", talk = 110010, npc2 = 110020, emoji2 = 0, content = "这个时候还说什么“身为勇者”的话啦！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 0, emoji2 = 0, content = "原谅无能的自己吧,毕竟你们已经尽力了。哈——", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "？？？", talk = 0, npc2 = 0, emoji2 = 0, content = "等一下,但他林！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110010, type = 2, emoji1 = 0, name = "伊菲", talk = 110010, npc2 = 110020, emoji2 = 0, content = "是谁在说话？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 0, emoji2 = 0, content = "你是……你……？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "？？？", talk = 0, npc2 = 0, emoji2 = 0, content = "没错,是我。因为现在身体十分虚弱的缘故,暂时无法与你见面,只能靠声音与你联系,非常抱歉。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 0, emoji2 = 0, content = "无所谓。不过,阻止我究竟是为了什么？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "？？？", talk = 0, npc2 = 0, emoji2 = 0, content = "这些人,这些通过传送之门迈向迦南世界的勇者,我还有用。请你不要为难他们。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "？？？", talk = 0, npc2 = 0, emoji2 = 0, content = "另外,我现在非常需要你的帮助。还记得曾经的誓言的话,尽快来找我吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 0, emoji2 = 0, content = "我明白了。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110030, type = 1, emoji1 = 0, name = "但他林", talk = 110030, npc2 = 0, emoji2 = 0, content = "哼哼,小家伙们,我们还会再见面的。希望那时候,你们不会像现在这样弱。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110010, type = 2, emoji1 = 0, name = "伊菲", talk = 110010, npc2 = 110020, emoji2 = 0, content = "走……走了？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 110020, npc2 = 110020, emoji2 = 0, content = "刚才那位……到底是什么人呢……？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 110020, emoji2 = 0, content = "爱拉忒酱也不知道吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 110010, type = 2, emoji1 = 0, name = "伊菲", talk = 110010, npc2 = 110020, emoji2 = 0, content = "哎呀,不要管那么多啦,这里的气氛怪怪的,我们还是快点离开吧！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },

		},
	},

	[2011] = {
		texts = {
			
			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "由于之前没有做什么准备就进入森林冒险的鲁莽决定\n我们在这片树林里被困了较长的一段时间", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "晴空", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 1, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 0, emoji2 = 0, content = "艳阳高照,真是让人舒服啊——", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 1, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 0, emoji2 = 0, content = "在这片森林里呆了这么久,总算走出来了。差不多要忘记人类是怎样生活了呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 1, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 0, emoji2 = 0, content = "如果能在王都旅店柔软的床上好好睡一觉的话,就别无他求啦！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 201020, npc2 = 201020, emoji2 = 0, content = "醒醒吧,对于伊菲君这样的人来说,软泥青苔与王都旅店的床并没什么区别的吧？喂,不要挡路啦,伊菲君！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 0, emoji2 = 0, content = "啊,痛痛痛痛痛……爱拉忒酱干嘛踩我的脚！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 201020, npc2 = 201020, emoji2 = 0, content = "是吗？哪有这回事啊？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 201020, emoji2 = 0, content = "爱拉忒酱！！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201020, emoji2 = 0, content = "唉……与其在这里争执,不如抓紧时间赶路吧两位。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 0, npc2 = 201020, emoji2 = 0, content = "就是嘛,抓紧时间赶路啦,幼稚鬼！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201020, emoji2 = 0, content = "不过,正如伊菲君所说,很久没看到这么好的阳光了呢。真是怀念啊。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201020, emoji2 = 0, content = "（因为之前缺乏充足的准备,就冒失地闯进了这片神秘的陌生森林,这段时间以来,大家都吃了不少苦头。）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201020, emoji2 = 0, content = "（不过,因此而收获的经验,比之枯燥单调的勇者训练,也丰富地几乎无法想象。）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201020, emoji2 = 0, content = "（伊菲君虽然偶尔还是会惹爱拉忒酱不开心,不过已经收敛了许多呢。）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201020, emoji2 = 0, content = "（爱拉忒酱较之以往也少了些锋芒,或许她只是一个戒备心有些强的小姑娘吧……）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 201020, emoji2 = 0, content = "$playername$sex:君|酱#,心事重重的是在想什么啊？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201020, emoji2 = 0, content = "什……什么？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 201020, emoji2 = 0, content = "$playername$sex:君|酱#的样子还真是让人担心啊……似乎连周围的异响都没听到呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 201020, emoji2 = 0, content = "这样松懈的警惕可是很危险的哟！离开这里之前,请躲在暗处的朋友无论如何出来见个面吧。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201020, emoji2 = 0, content = "什么……居然有人跟踪吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "爱拉忒", talk = 201020, npc2 = 201020, emoji2 = 0, content = "$playername$sex:君|酱#完全没有察觉吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 201020, emoji2 = 0, content = "再不出来的话,可要小心我手里的箭咯！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "？？？", talk = 0, npc2 = 201020, emoji2 = 0, content = "等……等一下！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201010, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201020, emoji2 = 0, content = "这个声音……好像是个小孩子呢。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 1, emoji1 = 0, name = "？？？", talk = 201030, npc2 = 0, emoji2 = 0, content = "请不要放箭,思莉嘉……思莉嘉没有恶意的。", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 201010, emoji2 = 0, content = "偷偷跟着我们很久了吧,究竟有什么目的,还不快说吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 201030, npc2 = 201010, emoji2 = 0, content = "我只是……只是想请各位帮帮我,救救我的家人！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 201020, npc2 = 201020, emoji2 = 0, content = "可以藏匿在山林里不被发现的小孩子好像也不简单呢,居然也需要别人帮助吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 201010, emoji2 = 0, content = "没有被发现的话,恐怕会一直跟下去吧？既然是想救人,为什么不早点露面呢？到底有什么企图？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 2, emoji1 = 0, name = "$playername", talk = 0, npc2 = 201010, emoji2 = 0, content = "这么小年纪,一个人在森林里很危险呢？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 2, emoji1 = 0, name = "思莉嘉", talk = 201030, npc2 = 201010, emoji2 = 0, content = "之所以到这里来,是要给得了重病被赶出村子的爷爷采药……", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 2, emoji1 = 0, name = "爱拉忒", talk = 201020, npc2 = 201020, emoji2 = 0, content = "为了照顾生病的家人,竟然不顾安危地独自在这片遍布魔物与魔神的危险山林里逗留吗？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 1, emoji1 = 0, name = "爱拉忒", talk = 201020, npc2 = 201020, emoji2 = 0, content = "谁会信这样的鬼话啊？", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 1, emoji1 = 0, name = "思莉嘉", talk = 201030, npc2 = 201020, emoji2 = 0, content = "虽然不知道该如何解释……不过,请相信我吧！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 201010, emoji2 = 0, content = "连谎话都懒得说吗,真是没有比这更愚蠢的辩白了！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 201030, type = 2, emoji1 = 0, name = "伊菲", talk = 201010, npc2 = 201010, emoji2 = 0, content = "只是个普通小孩的话,是绝对不会有任何危险的。如果是魔神的爪牙,这支箭就会给你足够的教训！", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },


			{ npc1 = 0, type = 6, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "（激烈的战斗过后……）", option1 = "", option2 = "", option3 = "", option4 = "", flipY1 = 0, flipY2 = 1, },

		},

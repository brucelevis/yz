--<<data_1500_ZhiyinTaskText 导表开始>>
data_1500_ZhiyinTaskText = {

	[1011] = {
		texts = {
			
			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "永夜即将降临,圣怒何以平息", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "吾,迦南卑微之臣,虔诚祈求", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "无所不能的先王所罗门陛下", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "听到臣民恳切的呼唤", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "愿您从异世无尽的长眠中苏醒", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 3, emoji1 = 0, name = "", talk = 0, npc2 = 0, emoji2 = 0, content = "带领您的子民,开辟希望与光明的出路", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "这里是什么地方……刚才……我……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 0, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "奇怪,怎么想不起来了……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "由传送之门而来的异界勇者吗？不错,很有活力的年轻人啊。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "我是这里的大祭司米利暗,有什么问题的话,阁下可以尽管问我。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "对不起,请问……这到底是怎么一回事？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "唉……这件事实在不宜声张,不过作为肩负着拯救迦南的使命的勇者,知道这些也是必须的。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "在如你所见的这座迦南的圣殿之中,原有两件令人忌惮的相克宝物。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "其一是我们的先祖与神定下的契约,其二是四百余年前,先圣王所罗门陛下降服并封印的魔神。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "魔神？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "是的,这些魔神,有七十二位之多。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "如果这个平衡没有被打破,迦南王国也许会一直平静下去。然而近日存放契约的圣约柜竟离奇消失。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "失去了制约的魔神们很快突破了封印,逃亡各地作乱,以致民不聊生。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1012] = {
		texts = {
			
			{ npc1 = 101015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "原来是这样啊……那么,我又是怎么到这里的呢？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 101015, type = 1, emoji1 = 0, name = "米利暗", talk = 101015, npc2 = 0, emoji2 = 0, content = "圣王所罗门陛下曾有遗训：于迦南危急时刻开启传送之门,陛下将重现在这片土地上,带领我们开辟希望之路。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1021] = {
		texts = {
			
			{ npc1 = 102015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "愿您从异世无尽的长眠中……苏醒……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "带领子民,开辟希望与光明的出路。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "那个人……祭司大人就是您吧？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "请叫我米利暗吧。虽然是大祭司,也不能承受被勇者们称作“大人”呢？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "那么,圣谕祭司说的所罗门王的转世“之一”又是怎样一回事呢？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "在传送之门开启的瞬间,圣殿里出现了数百位勇者。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "很抱歉,究竟为何如此目前仍无法明确,或许有某一位勇者大人是真正所罗门陛下的转世,或许大家都分别承担了陛下的力量,目前而言仍无法确定。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "不过,如果是得遇机缘的勇者大人的话,一定会找出导致这个情况的原因吧！", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "可是……就算是我这样从未接受过训练的人,也能成为一名合格的勇者吗？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "别担心,所谓勇者自然离不开冒险与战斗,通过不断地努力,一点一点强大起来。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1022] = {
		texts = {
			
			{ npc1 = 102015, type = 1, emoji1 = 0, name = "米利暗", talk = 102015, npc2 = 0, emoji2 = 0, content = "这样的改变可不是一朝一夕完成的,但是勇者大人的话,我相信不会有什么问题。而且,我的助手艾格娅也会竭尽全力辅助勇者们的。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1031] = {
		texts = {
			
			{ npc1 = 103025, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "那个……请问", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 103025, type = 1, emoji1 = 0, name = "初心者", talk = 103025, npc2 = 0, emoji2 = 0, content = "“那个那个”的烦死人了！", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 103025, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "对不起,请问,艾格娅大人是哪一位？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 103015, type = 1, emoji1 = 0, name = "艾格娅", talk = 103015, npc2 = 0, emoji2 = 0, content = "是有人叫我吗？久等啦,年轻的勇者们。你们的勇气真是令人赞赏,现在开始训练吧！", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 103015, type = 1, emoji1 = 0, name = "艾格娅", talk = 103015, npc2 = 0, emoji2 = 0, content = "作为迦南王国的吉祥物,【吉祥物】们拥有可爱形态的同时,也具有强大的灵力哟。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 103015, type = 1, emoji1 = 0, name = "艾格娅", talk = 103015, npc2 = 0, emoji2 = 0, content = "准备好了吗？挑战马上开始了噢！", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1032] = {
		texts = {
			
			{ npc1 = 103015, type = 4, emoji1 = 0, name = "艾格娅", talk = 103015, npc2 = 0, emoji2 = 0, content = "非常好,接下来等待你们的,将是更强悍的对手。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1041] = {
		texts = {
			
			{ npc1 = 104015, type = 1, emoji1 = 0, name = "艾格娅", talk = 104015, npc2 = 0, emoji2 = 0, content = "只靠粗拳笨腿在战斗中是无法取胜的,学习并掌握技能也是至关重要的事。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 104015, type = 1, emoji1 = 0, name = "艾格娅", talk = 104015, npc2 = 0, emoji2 = 0, content = "已经掌握了这一诀窍的话,就请迎接新的挑战吧。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 104015, type = 1, emoji1 = 0, name = "艾格娅", talk = 104015, npc2 = 0, emoji2 = 0, content = "不过,虽然是刚刚加入的勇者,在比试中失败也是很难看的。就由刚才那位勇者来多你的帮手好啦。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 104015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "刚才那位……初心者？不太好说话的样子呢……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 104015, type = 1, emoji1 = 0, name = "艾格娅", talk = 104015, npc2 = 0, emoji2 = 0, content = "事关勇者大人的威严,怎么能为这点小事而退缩呢？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 104015, type = 1, emoji1 = 0, name = "艾格娅", talk = 104015, npc2 = 0, emoji2 = 0, content = "准备好了吗？挑战马上开始了噢！", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1042] = {
		texts = {
			
			{ npc1 = 104015, type = 1, emoji1 = 0, name = "艾格娅", talk = 104015, npc2 = 0, emoji2 = 0, content = "到目前为止,你们的训练已经全部结束了,恭喜各位。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1051] = {
		texts = {
			
			{ npc1 = 105015, type = 1, emoji1 = 0, name = "米利暗", talk = 105015, npc2 = 0, emoji2 = 0, content = "果然很有天赋,不愧是吾王所罗门陛下的转世之人呐。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 105015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "被这样夸赞,真是叫人不好意思……", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 105015, type = 1, emoji1 = 0, name = "米利暗", talk = 105015, npc2 = 0, emoji2 = 0, content = "想必在之前的切磋中你已注意到,对手的招式均有不同套路配以相应的武器了吧。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 105015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "没错。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 105015, type = 1, emoji1 = 0, name = "米利暗", talk = 105015, npc2 = 0, emoji2 = 0, content = "他们也是来自不同职业的勇者呢。所谓职业,就是勇者大人们在未来的冒险中不断努力的方向啊。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 105015, type = 1, emoji1 = 0, name = "米利暗", talk = 105015, npc2 = 0, emoji2 = 0, content = "不同职业赋予勇者在力量方面有所差异。在未来的团队协作中,勇者们也要依据职业特点发挥相应的作用啊。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 105015, type = 1, emoji1 = 0, name = "米利暗", talk = 105015, npc2 = 0, emoji2 = 0, content = "选择了适合自己的职业,勇者们也好,我们的迦南王国也好,未来都将是充满希望的。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 105015, type = 1, emoji1 = 0, name = "米利暗", talk = 105015, npc2 = 0, emoji2 = 0, content = "希望你做出选择后能就坚定不移地走下去。完成转职后,前往勇者登记处办理登记之后,就是迦南王国登记在册的正式勇者啦。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1061] = {
		texts = {
			
			{ npc1 = 106015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "请问加入勇者注册是在这里登记吗？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 106015, type = 1, emoji1 = 0, name = "登记官", talk = 106015, npc2 = 0, emoji2 = 0, content = "是呢。看来是一位非常优秀的勇者啊。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 106015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "能得到您的认可,真是不胜感激。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 106015, type = 1, emoji1 = 0, name = "登记官", talk = 106015, npc2 = 0, emoji2 = 0, content = "将你的名字写在这里,王国的荣耀将与你同在！", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1062] = {
		texts = {
			
			{ npc1 = 106015, type = 1, emoji1 = 0, name = "登记官", talk = 106015, npc2 = 0, emoji2 = 0, content = "“$playername”（君或酱）吗？这是你的勇者证明。你可以回到圣殿向大祭司大人询问接下来的历练安排。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1071] = {
		texts = {
			
			{ npc1 = 107015, type = 1, emoji1 = 0, name = "米利暗", talk = 107015, npc2 = 0, emoji2 = 0, content = "$playerjob ？的确很适合你。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 107015, type = 1, emoji1 = 0, name = "米利暗", talk = 107015, npc2 = 0, emoji2 = 0, content = "对于勇者来说,在冒险中不断提升自己的实力是非常必要的。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 107015, type = 1, emoji1 = 0, name = "米利暗", talk = 107015, npc2 = 0, emoji2 = 0, content = "不过出门在外,一己之力显然是不够。勇者的冒险从来不是某一个人的故事,尽快找到投契的队友,旅程会更加安全而有趣呢。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 107015, type = 1, emoji1 = 0, name = "米利暗", talk = 107015, npc2 = 0, emoji2 = 0, content = "不同职业的队友相互扶持,也可以有效提高冒险的效率。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 107015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "那么米利暗大人,怎样才能找到不同职业的队友呢？", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 107015, type = 1, emoji1 = 0, name = "米利暗", talk = 107015, npc2 = 0, emoji2 = 0, content = "王国为了方便勇者们联络和互助,允许大家自行组建公会。在那里,你一定会找到志同道合的战友。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1072] = {
		texts = {
			
			{ npc1 = 107015, type = 1, emoji1 = 0, name = "米利暗", talk = 107015, npc2 = 0, emoji2 = 0, content = "越早加入公会,对勇者们的成长越有利。希望你能尽快找到适合自己的集体和伙伴。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 107015, type = 1, emoji1 = 0, name = "米利暗", talk = 107015, npc2 = 0, emoji2 = 0, content = "当然,在旅程中结识朋友也是很不错的选择。并肩作战的友谊可是最难能可贵的。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1081] = {
		texts = {
			
			{ npc1 = 108015, type = 1, emoji1 = 0, name = "米利暗", talk = 108015, npc2 = 0, emoji2 = 0, content = "面对冒险之路上未知的的风波和危险,仅仅有强大的武力是不够的。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 108015, type = 1, emoji1 = 0, name = "米利暗", talk = 108015, npc2 = 0, emoji2 = 0, content = "勇者们必须足够坚定,才能不为外物动摇,排除万难抵达光明的未来。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 108015, type = 1, emoji1 = 0, name = "米利暗", talk = 108015, npc2 = 0, emoji2 = 0, content = "在你正式加入冒险之前,我希望你你可以接受我安排的试炼,以证明自己已具备足够的能力。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 108015, type = 1, emoji1 = 0, name = "$playername", talk = 0, npc2 = 0, emoji2 = 0, content = "请大祭司赐教。", option1 = "", option2 = "", option3 = "", option4 = "", },


			{ npc1 = 108015, type = 1, emoji1 = 0, name = "米利暗", talk = 108015, npc2 = 0, emoji2 = 0, content = "也许在未来的旅途中,你们要经常与这些怪物打交道,所以要留神喽。", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

	[1082] = {
		texts = {
			
			{ npc1 = 108025, type = 1, emoji1 = 0, name = "艾格娅", talk = 108025, npc2 = 0, emoji2 = 0, content = "非常精彩！不知道我有没有机会和大祭司大人看中的勇者比试一番呢？", option1 = "", option2 = "", option3 = "", option4 = "", },

		},
	},

